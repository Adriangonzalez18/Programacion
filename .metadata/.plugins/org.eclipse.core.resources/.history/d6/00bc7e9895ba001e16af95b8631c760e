import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;

public class Taller {
	
	private Map<String,Coche> coche = new HashMap<String,Coche>();
	String matricula;

	public Taller(Map<String, Coche> coche) {
		this.coche = coche;
	}

	public Map<String, Coche> getCoches() {
		return coche;
	}

	public void setCoches(Map<String, Coche> coche) {
		this.coche = coche;
	}

	@Override
	public String toString() {
		return " \nCoches " + coche;
	}
	
	public boolean anadeElemento (coche  coches ) {
		
		coches.put(matricula , new Coche (coches.getColor(),coches.getMarca()));
		
		return false;
	
	}
	

}
