import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;

public class Taller {
	
	private Map<String,Coche> coches = new HashMap<String,Coche>();
	String matricula;

	public Taller(Map<String, Coche> coches) {
		this.coches = coches;
	}

	public Map<String, Coche> getCoches() {
		return coches;
	}

	public void setCoches(Map<String, Coche> coches) {
		this.coches = coches;
	}

	@Override
	public String toString() {
		return " \nCoches " + coches;
	}
	
	public boolean anadeElemento (Coche coches) {
	
		Coche coches1 = new Coche("azucar","hola");
		
		coches.put(coches1);
		
		return false;
	
	}
	

}
