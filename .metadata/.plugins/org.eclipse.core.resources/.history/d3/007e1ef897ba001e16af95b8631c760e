import java.util.HashMap;
import java.util.Map;

public class Taller {
	
	private Map<String,Coche> coche = new HashMap<String,Coche>();
	String matricula;

	public Taller(Map<String, Coche> coche) {
		this.coche = coche;
	}

	public Map<String, Coche> getCoches() {
		return coche;
	}

	public void setCoches(Map<String, Coche> coche) {
		this.coche = coche;
	}

	@Override
	public String toString() {
		return " \nCoches " + coche;
	}
	
	public void anadeElemento (String color,String marca, String maticula ) {
		
		if(coche.containsKey(maticula)) {
			System.out.println("La matr√≠cula ya esta registrada");
		}
		
		else {
			coche.put(matricula , new Coche (color,marca ));
		}
		
	
	}
	

}
